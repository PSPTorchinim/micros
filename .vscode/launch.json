{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Run All Services",
      "configurations": [
        "Launch CompanyAPI",
        "Launch DJHostGateway",
        "Launch EquipmentAPI",
        "Launch IdentityAPI",
        "Launch MusicAPI",
        "Launch PartyAPI",
        "Launch DocumentsAPI",
        "Launch MailingAPI",
        "Launch DJPanel"
      ]
    }
  ],
  "configurations": [
    {
      "command": "npm run start:dev",
      "name": "Launch DJPanel",
      "request": "launch",
      "type": "node-terminal",
      "localRoot": "${workspaceFolder}/Frontends/dj-panel"
    },
    {
      "name": "Launch CompanyAPI",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build CompanyAPI",
      "program": "${workspaceFolder}/Services/CompanyAPI/bin/Debug/net9.0/CompanyAPI.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Services/CompanyAPI",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/swagger"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "launchSettingsProfile": "http"
    },
    {
      "name": "Launch DJHostGateway",
      "type": "coreclr",
