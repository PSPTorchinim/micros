{
  "TokenConfiguration": {
    "Audience": "JWTServicePostmanClient",
    "Issuer": "JWTServiceAccessToken",
    "RequireHttpsMetadata": true,
    "SaveToken": true,
    "TokenExpireTime": "10",
    "ValidateAudience": false,
    "ValidateIssuer": false,
    "EnableSSL": false,
    "RefreshTokenExpireTime": "2592000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug",
      "Microsoft": "Debug",
      "System": "Debug"
    }
  },
  "AllowedHosts": "*"
}
